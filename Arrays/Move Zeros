//using 2 pointer technique
public void moveZeroes(int[] nums) {

        int j = 0;

        for(int i=0;i<nums.length;i++){
            if(nums[i] != 0){
                //swap with j 
                int temp = nums[i];
                nums[i] = nums[j];
                nums[j] = temp;
                j++;
            }
        }

    }
